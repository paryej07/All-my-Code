// Insert Element at the end of array.
#include<stdio.h>
void main(){
    int n,ele;
    printf("Enter the size of the array:");
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++){
        printf("Enter the element[%d]:",(i+1));
        scanf("%d",&arr[i]);
    }
    printf("Enter the element to add in the end :");
    scanf("%d",&ele);
    arr[n]=ele;
    n++;
    printf("New Array:\n");
    for(int i=0;i<n;i++){
        printf(" %d",arr[i]);
    }
}
